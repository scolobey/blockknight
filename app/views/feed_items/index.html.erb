<p id="notice"><%= notice %></p>

<h1>Feed Items</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Image</th>
      <th>Approved</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feed_items.each do |feed_item| %>
      <tr>
        <td><%= feed_item.title %></td>
        <td><%= feed_item.description %></td>
        <td><img src=<%= feed_item.image %>></td>
        <td><%= feed_item.approved %></td>
        <td><%= feed_item.content %></td>
        <td><%= link_to 'Show', feed_item %></td>
        <td><%= link_to 'Edit', edit_feed_item_path(feed_item) %></td>
        <td><%= link_to 'Destroy', feed_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feed Item', new_feed_item_path %>
